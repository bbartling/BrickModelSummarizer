@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix ex: <http://example.com/building#> .
@prefix ns1: <http://example.com/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ref: <https://brickschema.org/schema/Brick/ref#> .

ex:AHU1_Eff_StaticSPt a <brick:Supply_Air_Static_Pressure_Setpoint> ;
    brick:isPointOf ex:AHU1 ;
    ref:hasExternalReference <http://example.com/timeseries#AHU1_Eff_StaticSPt_ref> .

ex:AHU1_SaFanSpeedAO_value_ a <brick:Supply_Fan_Speed_Command> ;
    brick:isPointOf ex:AHU1 ;
    ref:hasExternalReference <http://example.com/timeseries#AHU1_SaFanSpeedAO_value__ref> .

ex:AHU1_SaStatic_value_in_wc a <brick:Supply_Air_Static_Pressure_Sensor> ;
    brick:isPointOf ex:AHU1 ;
    ref:hasExternalReference <http://example.com/timeseries#AHU1_SaStatic_value_in_wc_ref> .

ex:AHU1_Supply_Fan a brick:Supply_Fan ;
    brick:isPartOf ex:AHU1 .

<http://example.com/timeseries#AHU1_Eff_StaticSPt_ref> a ref:TimeseriesReference ;
    ref:hasTimeseriesId "AHU1_Eff_StaticSPt" ;
    ref:storedAt ns1:building_data.csv .

<http://example.com/timeseries#AHU1_SaFanSpeedAO_value__ref> a ref:TimeseriesReference ;
    ref:hasTimeseriesId "AHU1_SaFanSpeedAO_value (%)" ;
    ref:storedAt ns1:building_data.csv .

<http://example.com/timeseries#AHU1_SaStatic_value_in_wc_ref> a ref:TimeseriesReference ;
    ref:hasTimeseriesId "AHU1_SaStatic_value (in/wc)" ;
    ref:storedAt ns1:building_data.csv .

ns1:building_data.csv a ref:Database ;
    rdfs:label "building_data.csv Timeseries Storage" ;
    ns1:connstring "building_data.csv" .

ex:AHU1 a brick:Air_Handling_Unit ;
    brick:hasPart ex:AHU1_Supply_Fan ;
    brick:hasPoint ex:AHU1_Eff_StaticSPt,
        ex:AHU1_SaFanSpeedAO_value_,
        ex:AHU1_SaStatic_value_in_wc .

